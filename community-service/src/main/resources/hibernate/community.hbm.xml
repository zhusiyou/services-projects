<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="live.betterman.core.entity.Community" table="b_garden" lazy="true">
   		<cache usage="read-write"/>
	    <id name="id" column="id" type="java.lang.String">
            <generator class="assigned"></generator>
        </id>
		<property name="name" column="gardenName" type="java.lang.String"/>
        <property name="aliasOne" column="asNameOne" type="java.lang.String"/>
        <property name="aliasTwo" column="asNameTwo" type="java.lang.String"/>
        <property name="aliasThree" column="aliasThree" type="java.lang.String"/>

    </class>
</hibernate-mapping>